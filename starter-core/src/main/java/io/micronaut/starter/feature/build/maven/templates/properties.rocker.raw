@import io.micronaut.starter.build.Property
@import java.util.List

@args (List<Property> properties)

@if (properties != null && !properties.isEmpty()) {
  <properties>
@for (Property prop : properties) {
  @if (prop.isComment()) {
    <!--@prop.getComment()-->
  } else {
    <@(prop.getKey())>@(prop.getValue())</@(prop.getKey())>
  }
}
  </properties>
}
